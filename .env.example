# contract-agent environment variables
# Copy this file to .env.local and fill in the values

# LLM Provider API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_AI_API_KEY=
MISTRAL_API_KEY=
LLAMA_API_KEY=

# LLM Provider Endpoints (optional - defaults are used if not set)
OPENAI_BASE_URL=https://api.openai.com/v1
ANTHROPIC_BASE_URL=https://api.anthropic.com
GOOGLE_AI_BASE_URL=https://generativelanguage.googleapis.com
MISTRAL_BASE_URL=https://api.mistral.ai/v1
LLAMA_BASE_URL=https://api.together.xyz/v1

# Database & Storage (Supabase)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-sa-east-1.pooler.supabase.com:6543/postgres
SUPABASE_URL=https://[project-ref].supabase.co
SUPABASE_SERVICE_KEY=eyJ...

# Job Configuration
JOB_EXPIRATION_HOURS=24
JOB_CLEANUP_INTERVAL_MINUTES=30

# LLM Configuration
LLM_TIMEOUT_MS=90000
LLM_MAX_RETRIES=2

# Tenant (MVP single-tenant)
DEFAULT_TENANT_ID=default
